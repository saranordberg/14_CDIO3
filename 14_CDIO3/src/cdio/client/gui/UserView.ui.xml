				<g:VerticalPanel ui:field="content" width="94%" spacing="5" >
				<g:Label ui:field="lbl" text="Loading..." />
			</g:VerticalPanel>
			</g:Cell>
			<g:Cell horizontalAlignment="ALIGN_RIGHT">
				<g:Button ui:field="button" width="35px" height="35px"
					styleName="round_button">+</g:Button>
			</g:Cell>
		</g:VerticalPanel>

	</g:HTMLPanel>
</ui:UiBinder> 
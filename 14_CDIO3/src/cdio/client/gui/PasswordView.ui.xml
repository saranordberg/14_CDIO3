<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	    .scrollable {
	      height: 400px;
	      width: 250px;
	      border: 1px solid #ccc;
	      text-align: left;
	    }
	    
	    .contactFormCell {
	      padding-left: 20px;
	    }
	  </ui:style>
	
	<g:HTMLPanel>
		<g:HorizontalPanel width="400px">
			<g:VerticalPanel horizontalAlignment="ALIGN_LEFT" styleName="inner2" width="200px" spacing="0">
				<g:Cell width="100%" horizontalAlignment="ALIGN_CENTER">
					<g:Label ui:field="userIdLabel" width="90%" horizontalAlignment="ALIGN_LEFT" >Gammelt kodeord:</g:Label>
				</g:Cell>
				<g:Cell width="100%" horizontalAlignment="ALIGN_CENTER">
					<g:TextBox  ui:field="oldPw" width="90%"></g:TextBox>
				</g:Cell>
				
				<g:Cell width="100%" horizontalAlignment="ALIGN_CENTER">
					<g:Label ui:field="firtNameLabel" width="90%" horizontalAlignment="ALIGN_LEFT">Nyt kodeord:</g:Label>
				</g:Cell>
				<g:Cell width="100%" horizontalAlignment="ALIGN_CENTER">
					<g:TextBox ui:field="newPw" width="90%"/>
				</g:Cell>
				
				<g:Cell width="100%" horizontalAlignment="ALIGN_CENTER">
					<g:Label ui:field="lastNameLabel" width="90%" horizontalAlignment="ALIGN_LEFT">Gentag nyt kodeord:</g:Label>
				</g:Cell>
				<g:Cell width="100%" horizontalAlignment="ALIGN_CENTER">
					<g:TextBox ui:field="newPw2" width="90%"/>
				</g:Cell>
				<g:Cell width="100%" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_BOTTOM">
					<g:Button ui:field="actionButton" width="190px" height="35px" styleName="rect_button">Gem</g:Button>
				</g:Cell>
			</g:VerticalPanel>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 
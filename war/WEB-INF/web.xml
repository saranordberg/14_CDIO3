<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
 	<servlet-name>operatorServlet</servlet-name>
 	<servlet-class>cdio.service.implementation.OperatorServiceImpl</servlet-class>
 </servlet>
 
 <servlet-mapping>
 	<servlet-name>operatorServlet</servlet-name>
 	<url-pattern>/Group14_CDIO3/operatorService</url-pattern>
 </servlet-mapping>
  
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Group14_CDIO3.html</welcome-file>
  </welcome-file-list>

</web-app>
